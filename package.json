{
  "name": "proyecto-mfp-back",
  "version": "1.0.0",
  "description": "Homy, app de reservas de inmuebles",
  "main": "index.js",
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "scripts": {
    "dev": "nodemon -r dotenv/config app.js",
    "db:create": "sequelize-cli db:create",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:drop": "sequelize-cli db:drop",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:start": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "eslint",
    "sonarqube": "sonar-scanner",
    "test": "cross-env NODE_ENV=test jest",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:start:prod": "cross-env NODE_ENV=production npm run db:start",
    "prod": "cross-env NODE_ENV=production nodemon -r dotenv/config app.js",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MFP-2021-II/Proyecto-MFP-Back.git"
  },
  "author": "Railly Hugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MFP-2021-II/Proyecto-MFP-Back/issues"
  },
  "homepage": "https://github.com/MFP-2021-II/Proyecto-MFP-Back#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.27.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.9.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.3.0",
    "sonar-scanner": "^3.1.0",
    "supertest": "^6.1.6"
  }
}
