{
  "name": "proyecto-mfp-back",
  "version": "1.0.0",
  "description": "Homy, app de reservas de inmuebles",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config app.js",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:drop": "sequelize-cli db:drop",
    "start": "node app.js",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MFP-2021-II/Proyecto-MFP-Back.git"
  },
  "author": "Railly Hugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MFP-2021-II/Proyecto-MFP-Back/issues"
  },
  "homepage": "https://github.com/MFP-2021-II/Proyecto-MFP-Back#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.9.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.3.0"
  }
}
